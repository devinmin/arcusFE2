{
  "name": "arcus-zo-backend",
  "version": "1.0.0",
  "description": "Arcus Zo - Unified Marketing Intelligence Platform",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/workers/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "worker": "node dist/workers/worker.js",
    "migrate": "tsx src/database/migrate.ts",
    "seed": "tsx src/database/seed.ts",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/__tests__ --reporter=verbose",
    "lint": "eslint 'src/**/*.ts'",
    "lint:strict": "eslint 'src/**/*.ts' --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "e2e:nike": "tsx src/scripts/e2e_nike_jordan.ts",
    "e2e:max": "tsx src/scripts/runMaxE2e.ts",
    "test:division": "tsx src/scripts/runDivisionTest.ts",
    "audit:prod": "npm audit --omit=dev",
    "health": "node -e \"require('http').get('http://localhost:3000/health',r=>{console.log('HEALTH',r.statusCode);})\"",
    "test:magic": "tsx src/scripts/test_magic_orchestrator.ts",
    "e2e:full": "tsx src/scripts/soupToNutsE2e.ts",
    "check:wiring": "tsx src/scripts/quickWiringCheck.ts",
    "test:tiered": "tsx src/scripts/tieredAgencyTest.ts",
    "test:phase0": "vitest run src/tests/execution-layer/phase0 --reporter=verbose",
    "test:phase1": "vitest run src/tests/execution-layer/phase1 --reporter=verbose",
    "test:phase7": "vitest run src/tests/execution-layer/phase7 --reporter=verbose",
    "test:execution": "vitest run src/tests/execution-layer --reporter=verbose",
    "test:exhaustive": "npx tsx src/scripts/test_exhaustive_arcus.ts",
    "test:usecases": "npx tsx src/scripts/test_full_use_cases.ts",
    "test:full": "npx tsx src/scripts/run_all_tests.ts",
    "test:io": "npx tsx src/scripts/test_io_comprehensive.ts",
    "test:io:agents": "npx tsx src/scripts/test_io_agents.ts",
    "test:io:departments": "npx tsx src/scripts/test_io_departments.ts",
    "test:io:cross": "npx tsx src/scripts/test_io_cross_department.ts",
    "test:io:swarm": "npx tsx src/scripts/test_io_swarm.ts"
  },
